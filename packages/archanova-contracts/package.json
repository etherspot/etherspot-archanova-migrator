{
  "name": "@etherspot/archanova-contracts",
  "version": "1.0.0",
  "private": true,
  "description": "ETHERspot archanova contracts",
  "license": "UNLICENSED",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run build:dist",
    "build:dist": "hardhat build-dist",
    "compile": "hardhat compile",
    "deploy:localA": "hardhat deploy --network localA",
    "deploy:localB": "hardhat deploy --network localB",
    "lint": "npm run lint:ts && npm run lint:sol",
    "lint:sol": "solhint ./src/**/*.sol ./src/**/**/*.sol",
    "lint:ts": "eslint \"{deploy,extensions}/**/*.ts\"",
    "lint:ts-fix": "npm run lint -- --fix",
    "start:node": "hardhat node --no-deploy"
  },
  "dependencies": {
    "@ensdomains/ens": "0.4.0",
    "@etherspot/contracts": "1.0.0",
    "openzeppelin-solidity": "2.1.2",
    "solidity-bytes-utils": "0.0.8"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "5.0.0",
    "@types/fs-extra": "8.1.0",
    "@types/mocha": "7.0.2",
    "@types/node": "13.9.2",
    "ethers": "5.0.24",
    "fs-extra": "9.0.0",
    "hardhat": "2.0.4",
    "hardhat-deploy": "0.7.0-beta.37",
    "hardhat-deploy-ethers": "0.3.0-beta.7",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  }
}
