    "deploy:localA": "hardhat deploy --network localA",
    "deploy:localB": "hardhat deploy --network localB"
  },
  "dependencies": {},
  "devDependencies": {
    "@etherspot/archanova-contracts-common": "1.0.0",
    "@etherspot/contracts": "1.2.0",
    "@types/mocha": "7.0.2",
    "@types/node": "13.9.2",
    "ethers": "5.0.24",
    "hardhat": "2.1.1",
    "rimraf": "3.0.2",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  }
}
