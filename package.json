    "contracts-platform:compile": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-platform compile",
    "contracts-platform:build": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-platform build",
    "contracts-platform:deploy:localA": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-platform deploy:localA",
    "contracts-platform:deploy:localB": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-platform deploy:localB",
    "migrator:build": "lerna run --no-prefix --stream --scope @etherspot/archanova-migrator build",
    "migrator:compile-watch": "lerna run --no-prefix --stream --scope @etherspot/archanova-migrator compile-watch",
    "migrator:benchmark": "lerna run --no-prefix --scope @etherspot/archanova-migrator-benchmark start",
    "reset": "lerna run --no-prefix --stream reset",
    "deploy:localA": "lerna run --no-prefix --stream deploy:localA",
    "deploy:localB": "lerna run --no-prefix --stream deploy:localB",
    "build": "lerna run --no-prefix --stream build"
  },
  "devDependencies": {
    "@types/node": "13.9.2",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "lerna": "3.22.1",
    "prettier": "1.19.1",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.7"
  }
