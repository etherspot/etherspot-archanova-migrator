{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "description": "Etherspot archanova migrator",
  "license": "MIT",
  "scripts": {
    "lerna:clean": "lerna clean --yes",
    "lerna:bootstrap": "lerna bootstrap",
    "contracts-common:build": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-common build",
    "contracts-common:compile-watch": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-common compile-watch",
    "contracts-migrator:reset": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-migrator reset",
    "contracts-migrator:compile": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-migrator compile",
    "contracts-migrator:build": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-migrator build",
    "contracts-migrator:deploy:mainnet": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-migrator deploy:mainnet",
    "contracts-migrator:deploy:ropsten": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-migrator deploy:ropsten",
    "contracts-migrator:deploy:kovan": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-migrator deploy:kovan",
    "contracts-migrator:deploy:localA": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-migrator deploy:localA",
    "contracts-migrator:deploy:localB": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-migrator deploy:localB",
    "contracts-migrator:verify:mainnet": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-migrator verify:mainnet",
    "contracts-migrator:verify:ropsten": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-migrator verify:ropsten",
    "contracts-migrator:verify:kovan": "lerna run --no-prefix --stream --scope @etherspot/archanova-contracts-migrator verify:kovan",
